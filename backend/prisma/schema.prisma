generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}


model Person {
  id Int @id @default(autoincrement())
  name String
  xMin Float
  xMax Float 
  yMin Float
  yMax Float

  players Player[]

}

model Player {
  id Int @id @default(autoincrement())
  start Float
  end Float
  
  found Person[]

}
