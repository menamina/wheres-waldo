generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}


model Person {
  id Int @id @default(autoincrement())
  name String
  xMin Int
  xMax Int 
  yMin Int
  yMax Int

  players Player[]

}

model Player {
  id Int @id @default(autoincrement())
  start Int
  end Int
  
  found Person[]

}